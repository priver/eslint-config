{
  "name": "eslint-config-priver",
  "version": "1.2.2",
  "description": "Shareable config for eslint",
  "main": "index.js",
  "files": [
    "rules",
    "base.js",
    "index.js",
    "node.js",
    "react.js"
  ],
  "scripts": {
    "lint:js": "eslint .",
    "lint": "yarn run --silent lint:js",
    "unused:index": "eslint-find-rules --unused ./index.js",
    "unused:react": "eslint-find-rules --unused ./react.js",
    "unused:node": "eslint-find-rules --unused ./node.js",
    "unused": "yarn run --silent unused:index && yarn run --silent unused:react && yarn run --silent unused:node",
    "test": "yarn run --silent lint && yarn run --silent unused"
  },
  "homepage": "https://github.com/priver/eslint-config-priver",
  "bugs": "https://github.com/priver/eslint-config-priver/issues",
  "repository": "priver/eslint-config-priver",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "priver",
    "javascript",
    "styleguide"
  ],
  "author": "Mikhail Priver <m.priver@gmail.com> (https://github.com/priver)",
  "license": "MIT",
  "dependencies": {
    "eslint-restricted-globals": "^0.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.10.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.4.0"
  },
  "peerDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.10.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.4.0"
  },
  "engines": {
    "node": ">=6"
  }
}
